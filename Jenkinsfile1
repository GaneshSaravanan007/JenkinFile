pipeline {
    agent any

    stages {
        stage('editing') {
            steps {
                script{
                  sh '''
                      SHA=$(curl -s \
https://api.github.com/repos/GaneshSaravanan007/JenkinFile/contents/hello.txt \
| jq -r .sha)
echo $SHA

                    '''
                }
            }
        }
    }
}
